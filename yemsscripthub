-- Scripts:

local function PHRC_fake_script() -- YemanicasScriptHub.Manager 
	local script = Instance.new('LocalScript', YemanicasScriptHub)

	
end
coroutine.wrap(PHRC_fake_script)()
local function MDOUZLI_fake_script() -- Minimize.Script 
	local script = Instance.new('Script', Minimize)

	local status = 0
	script.Parent.MouseButton1Click:Connect(function()
		if status == 0 then
			script.Parent.Parent.Mainframe.Visible = false
			script.Parent.Text = "+"
			status = 1
		elseif status == 1 then
			script.Parent.Parent.Mainframe.Visible = true
			script.Parent.Text = "-"
			status = 0
		end
	end)
end
coroutine.wrap(MDOUZLI_fake_script)()
