local function XUDD_fake_script() -- TextLabel.Script 
	local script = Instance.new('Script', TextLabel)

	local status = 0
	script.Parent.MouseEnter:Connect(function()
		script.Parent.Text = "Yemanica's Script Hub"
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent.Text = "Yem's Script Hub"
	end)
end
