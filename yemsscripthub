local function HGNZGID_fake_script() -- Mainframe.TabSwitcher 
	local script = Instance.new('LocalScript', Mainframe)

	script.Parent.ScriptsBtn.MouseButton1Click:Connect(function()
		script.Parent.SettingsTab.Visible = false
		script.Parent.ScriptsTab.Visible = true
	end)
	
	script.Parent.SettingsBtn.MouseButton1Click:Connect(function()
		script.Parent.SettingsTab.Visible = true
		script.Parent.ScriptsBtn.Visible = false
	end)
end
coroutine.wrap(HGNZGID_fake_script)()
