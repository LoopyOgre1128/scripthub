local function UEYC_fake_script() -- ScriptsTab.TabSwitcher 
	local script = Instance.new('LocalScript', ScriptsTab)

	local frame = script.Parent.ScrollingFrame
	local main = script.Parent
	
	frame.cocoHubBtn.MouseButton1Click:Connect(function()
		main.CocoHubFrame.Visible = true
		main.ZyrexHubFrame.Visible = false
		main.AntiAfkFrame.Visible = false
		main.InfiniteYieldFrame.Visible = false
		main["YHS Frame"].Visible = false
	end)
	
	frame.infiniteYield.MouseButton1Click:Connect(function()
		main.CocoHubFrame.Visible = false
		main.ZyrexHubFrame.Visible = false
		main.AntiAfkFrame.Visible = false
		main.InfiniteYieldFrame.Visible = true
		main["YHS Frame"].Visible = false
	end)
	
	frame.zyrexHubBtn.MouseButton1Click:Connect(function()
		main.CocoHubFrame.Visible = false
		main.ZyrexHubFrame.Visible = true
		main.AntiAfkFrame.Visible = false
		main.InfiniteYieldFrame.Visible = false
		main["YHS Frame"].Visible = false
	end)
	
	frame.antiAFK:Connect(function()
		main.CocoHubFrame.Visible = false
		main.ZyrexHubFrame.Visible = false
		main.AntiAfkFrame.Visible = true
		main.InfiniteYieldFrame.Visible = false
		main["YHS Frame"].Visible = false
	end)
	
	frame["Yem's Script Hub"].MouseButton1Click:Connect(function()
		main.CocoHubFrame.Visible = false
		main.ZyrexHubFrame.Visible = false
		main.AntiAfkFrame.Visible = false
		main.InfiniteYieldFrame.Visible = false
		main["YHS Frame"].Visible = true
	end)
end
coroutine.wrap(UEYC_fake_script)()
